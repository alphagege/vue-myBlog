{
  "name": "coframe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "buildMustache": "mustache mustache.json myTemplate.mustache>public/output.html"
  },
  "dependencies": {
    "animate.css": "^3.7.2",
    "axios": "^0.19.0",
    "codemirror": "^5.58.3",
    "core-js": "^2.6.5",
    "driver.js": "^0.9.7",
    "echarts": "^4.2.1",
    "echarts-gl": "^1.1.1",
    "element-ui": "^2.11.1",
    "html2canvas": "^1.0.0-rc.7",
    "js-cookie": "^2.2.1",
    "jspdf": "^2.2.0",
    "mustache": "^4.1.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "tinymce": "^4.9.2",
    "vue": "^2.6.10",
    "vue-3d-model": "^1.1.1",
    "vue-countupjs": "^1.0.0",
    "vue-picture-preview": "^2.0.1",
    "vue-router": "^3.1.2",
    "vue-video-player": "^5.0.2",
    "vue-waterfall-easy": "^2.4.4",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.1.1",
    "vuex-persistedstate": "^2.5.4",
    "zx-count": "^0.3.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.1",
    "compression-webpack-plugin": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.2.0",
    "svg-sprite-loader": "^4.1.6",
    "terser-webpack-plugin": "^1.4.1",
    "vue-pano": "^1.4.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": [
        "warn",
        {
          "vars": "local",
          "args": "none",
          "ignoreRestSiblings": false
        }
      ],
      "vue/no-parsing-error": [
        2,
        {
          "x-invalid-end-tag": false
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
